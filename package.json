{
  "version": "0.8.0-pre.124",
  "license": "CC-BY-4.0",
  "homepage": "https://macaw-ui.vercel.app/",
  "repository": {
    "type": "git",
    "url": "git://github.com/saleor/macaw-ui.git"
  },
  "bugs": {
    "url": "https://github.com/saleor/macaw-ui/issues"
  },
  "files": [
    "dist/*",
    "legacy/dist/*"
  ],
  "typesVersions": {
    "*": {
      "next": [
        "dist/index.d.ts"
      ],
      "next/theme": [
        "dist/theme/index.d.ts"
      ],
      "*": [
        "legacy/dist/types/*"
      ]
    }
  },
  "packageManager": "pnpm@8.6.11",
  "exports": {
    "./next": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./next/style": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    },
    "./next/theme": {
      "types": "./dist/theme/index.d.ts",
      "import": "./dist/theme.mjs",
      "require": "./dist/theme.js"
    },
    ".": {
      "types": "./legacy/dist/types/index.d.ts",
      "import": "./legacy/dist/esm/index.js",
      "require": "./legacy/dist/cjs/index.js"
    }
  },
  "keywords": [
    "saleor",
    "macaw",
    "macaw-ui",
    "react",
    "ui components"
  ],
  "engines": {
    "node": "16 || 18 || 20",
    "pnpm": ">=8"
  },
  "scripts": {
    "build:legacy": "cd legacy && npm run build",
    "build:next": "npm-run-all check-types:next && vite build",
    "bundle-visualizer": "VISUALIZER=true vite build",
    "build-storybook": "storybook build --docs",
    "watch": "vite build --watch",
    "lint:legacy": "cd legacy && npm run lint",
    "lint:next": "eslint 'src/**/*.@(tsx|ts|jsx|js)' --fix",
    "check-types:next": "tsc",
    "check-types:legacy": "cd legacy && npm run build-tsc",
    "test:legacy": "cd legacy && npm run test",
    "test:next": "vitest",
    "lint": "npm-run-all lint:*",
    "check-types": "npm-run-all check-types:*",
    "test": "npm-run-all test:*",
    "build": "npm-run-all build:*",
    "dev": "storybook dev -p 6006",
    "dev:docs": "storybook dev -p 6006 --docs",
    "make-release": "release-it pre",
    "release": "npm-run-all build make-release",
    "prepare": "is-ci || husky install",
    "chromatic": "chromatic --exit-zero-on-changes"
  },
  "name": "@saleor/macaw-ui",
  "description": "Saleor's UI component library",
  "author": "Saleor Commerce",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@release-it/conventional-changelog": "^7.0.0",
    "@storybook/addon-actions": "^7.2.1",
    "@storybook/addon-essentials": "^7.2.1",
    "@storybook/addon-interactions": "^7.2.1",
    "@storybook/addon-links": "^7.2.1",
    "@storybook/react": "^7.2.1",
    "@storybook/react-vite": "^7.2.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@types/lodash-es": "4.17.8",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "@vanilla-extract/css": "^1.12.0",
    "@vanilla-extract/dynamic": "^2.0.3",
    "@vanilla-extract/recipes": "^0.5.0",
    "@vanilla-extract/sprinkles": "^1.6.1",
    "@vanilla-extract/vite-plugin": "^3.8.2",
    "@vitejs/plugin-react": "^4.0.4",
    "chromatic": "^6.20.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.13",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "release-it": "^16.1.3",
    "require-from-string": "^2.0.2",
    "rollup-plugin-visualizer": "5.9.2",
    "storybook": "^7.2.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.8",
    "vite-plugin-dts": "^3.4.0",
    "vitest": "^0.34.1",
    "webpack": "^5.88.2"
  },
  "dependencies": {
    "@dessert-box/react": "^0.4.0",
    "@floating-ui/react-dom-interactions": "^0.5.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.5",
    "@radix-ui/react-popover": "^1.0.6",
    "@radix-ui/react-portal": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-tooltip": "^1.0.6",
    "@vanilla-extract/css-utils": "^0.1.3",
    "clsx": "^1.2.1",
    "downshift": "^6.1.12",
    "downshift7": "npm:downshift@7.6.0",
    "lodash-es": "^4.17.21",
    "react-inlinesvg": "^3.0.2"
  },
  "peerDependencies": {
    "@types/react": "^16.9.0 || ^17.0.0 || ^18.0.0",
    "@types/react-dom": "^16.9.0 || ^17.0.0 || ^18.0.0",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  }
}
