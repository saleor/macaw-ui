{
  "version": "0.8.0-pre.10",
  "license": "CC-BY-4.0",
  "homepage": "https://macaw-ui.vercel.app/",
  "repository": {
    "type": "git",
    "url": "git://github.com/saleor/macaw-ui.git"
  },
  "bugs": {
    "url": "https://github.com/saleor/macaw-ui/issues"
  },
  "type": "module",
  "files": [
    "dist/*",
    "legacy/dist/*"
  ],
  "typesVersions": {
    "*": {
      "next": [
        "dist/index.d.ts"
      ],
      "*": [
        "legacy/dist/types/*"
      ]
    }
  },
  "exports": {
    "./next": {
      "import": "./dist/macaw-ui.js",
      "require": "./dist/macaw-ui.umd.cjs"
    },
    "./next/style": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    },
    ".": {
      "import": "./legacy/dist/esm/index.js",
      "require": "./legacy/dist/cjs/index.js"
    }
  },
  "keywords": [
    "saleor",
    "macaw",
    "macaw-ui",
    "react",
    "ui components"
  ],
  "engines": {
    "node": ">=16 <19"
  },
  "scripts": {
    "build:legacy": "cd legacy && npm run build",
    "build:next": "npm-run-all check-types:next && vite build",
    "build-storybook": "cross-env NODE_OPTIONS=--openssl-legacy-provider build-storybook",
    "lint:legacy": "cd legacy && npm run lint",
    "lint:next": "eslint 'src/**/*.@(tsx|ts|jsx|js)' --fix",
    "check-types:next": "tsc",
    "check-types:legacy": "cd legacy && npm run build-tsc",
    "test:legacy": "cd legacy && npm run test",
    "test:next": "vitest",
    "lint": "npm-run-all lint:*",
    "check-types": "npm-run-all check-types:*",
    "test": "npm-run-all test:*",
    "build": "npm-run-all build:*",
    "dev": "cross-env NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006",
    "make-release": "release-it pre",
    "release": "npm-run-all build make-release",
    "prepare": "is-ci || husky install"
  },
  "name": "@saleor/macaw-ui",
  "description": "Saleor's UI component library",
  "author": "Saleor Commerce",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@dessert-box/react": "^0.4.0",
    "@radix-ui/react-accordion": "^1.0.1",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-vite": "^0.2.6",
    "@storybook/react": "^6.5.15",
    "@types/react": "^17.0.50",
    "@types/react-dom": "^17.0.17",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "@vanilla-extract/css": "^1.9.2",
    "@vanilla-extract/recipes": "^0.3.0",
    "@vanilla-extract/sprinkles": "^1.5.1",
    "@vanilla-extract/vite-plugin": "^3.7.0",
    "@vitejs/plugin-react": "^2.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "release-it": "^15.6.0",
    "require-from-string": "^2.0.2",
    "typescript": "^4.9.3",
    "vite": "^3.2.5",
    "vite-plugin-dts": "^1.7.1",
    "vitest": "^0.27.2",
    "webpack": "^5.75.0",
    "@radix-ui/react-dropdown-menu": "^2.0.2",
    "@radix-ui/react-dialog": "^1.0.2",
    "@vanilla-extract/dynamic": "^2.0.3"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  }
}
